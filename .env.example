# OpenCage Geocoding API
# Sign up at https://opencagedata.com to get your free API key
# Free tier: 2,500 requests/day
VITE_OPENCAGE_API_KEY=your_api_key_here

# Google Maps API
# Sign up at https://console.cloud.google.com to get your API key
# Enable Places API for address autocomplete
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Optional: Default Map Configuration
VITE_DEFAULT_LAT=0
VITE_DEFAULT_LNG=0
VITE_DEFAULT_ZOOM=2

# Optional: Analytics
VITE_GA_TRACKING_ID=
